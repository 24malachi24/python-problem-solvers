    "def dict_invert(d):\n",
    "    \n",
    "    invD = {}\n",
    "    for k, v in d.items():\n",
    "        invD[v] = invD.get(v, [])+[k]\n",
    "    return invD\n",
    "d= {1:10, 2:20 , 3:30}\n",
    "e = {1:10, 2:20 , 3:30, 4:30}\n",
    "f = {4:True, 2:True, 0:True}\n",
    "print(dict_invert(d))\n",
    "print(dict_invert(e))\n",
    "print(dict_invert(f))"
   ]
